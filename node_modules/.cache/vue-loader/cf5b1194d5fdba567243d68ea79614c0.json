},\n    methods: {\n        getLocalUrl() {\n            let e = document.createElement('input'),\n                t = window.location.href + '?utm_source=topicsarea&utm_medium=copybutton'\n            document.body.appendChild(e),\n                (e.value = t),\n                e.select(),\n                document.execCommand('copy'),\n                document.body.removeChild(e),\n                alert('網址複製成功!')\n        },\n\n        scrollTop() {\n            window.scrollTo({ top: 0, behavior: 'smooth' })\n        },\n\n        scroll() {\n            this.scrollY = window.scrollY\n        },\n    },\n    created() {\n        window.addEventListener('scroll', this.scroll)\n    },\n}\n</script>\n\n<style scoped>\n.layout_addition {\n    height: 100px;\n}\n\n.layout_grid {\n    grid-template-columns: 1fr 1fr 1fr;\n    max-width: 300px;\n    padding: 1rem;\n}\n\na,\nspan {\n    display: block;\n    margin: auto;\n    cursor: pointer;\n    filter: contrast(1);\n}\n\na:hover {\n    background-color: #ffffff00;\n}\n\n.addition {\n    position: fixed;\n    top: 5%;\n    right: 1px;\n    border-radius: 15px;\n    padding: 10px 5px;\n    background-color: #f7f7f5;\n    box-shadow: 1px 2px 3px black;\n    z-index: 999;\n}\n\n#social_line,\n#social_facebook,\n#social_link {\n    display: block;\n    background-size: 40px;\n    width: 40px;\n    height: 40px;\n\n    background-repeat: no-repeat;\n    border-radius: 10px;\n}\n\n#social_line {\n    border-radius: 10px;\n    background-image: url('../assets/icon_line.png');\n}\n\n#social_facebook {\n    border-radius: 10px;\n    background-image: url('../assets/icon_fb.png');\n}\n\n#social_link {\n    border-radius: 10px;\n    background-image: url('../assets/icon_share.png');\n}\n\n.fa-square-arrow-up {\n    position: fixed;\n    right: 1rem;\n    bottom: 4rem;\n    z-index: 9999;\n    color: #162237;\n    opacity: .8;\n}\n</style>\n"]}]}